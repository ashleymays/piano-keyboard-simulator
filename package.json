  "private": "true",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "npm run build",
    "build": "node server/server.js",
    "dev": "nodemon server/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ashleymays/piano-keyboard-simulator.git"
  },
  "keywords": [],
  "author": "",
