{
  "name": "piano-keyboard-simulator",
  "version": "6.0.0",
  "description": "A virtual piano keyboard that can be played with a computer keyboard, a mouse, or touch inputs. Users can play one of four different instrument sounds, mimic the sustain and soften pedals on a grand piano, record their playing, and download recordings as an .mp3 file.",
  "main": "src/index.jsx",
  "private": "true",
  "type": "module",
  "scripts": {
    "start": "npm run format && vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,jsx,cjs,json}' --config ./.prettierrc.yaml"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ashleymays/piano-keyboard-simulator.git"
  },
  "keywords": [],
  "author": "Ashley Mays",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ashleymays/piano-keyboard-simulator/issues"
  },
  "homepage": "https://github.com/ashleymays/piano-keyboard-simulator#readme",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.69.5",
    "tone": "^14.7.77"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.2.1",
    "vite": "^5.0.10",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^1.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
